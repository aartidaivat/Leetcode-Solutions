class Solution {
    public void moveZeroes(int[] nums) {
        for(int i=0, j=i+1; j<nums.length; ) {
            if(nums[i]==0 && nums[j]!=0) {
                nums[i]=nums[j];
                nums[j]=0;
                i++; j++;
            }
            else if(nums[i]!=0) {
                i++; j++;
            }
            else {
                j++;
            }
        }
    }
}
