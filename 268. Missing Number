class Solution {
    public int missingNumber(int[] nums) {
        Set<Integer> set= new HashSet<>();
        for(int i=0; i<nums.length; i++) {
            set.add(nums[i]);
        }
        int i=0;
        for(i=0; i<nums.length; i++) {
            if(!set.contains(i)) {
                return i;
            }
        }
        return i;
    }
}
