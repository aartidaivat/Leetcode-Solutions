class Solution {
    public void reverseString(char[] s) {
        int i=0, j=s.length-1;
        for(i=0,j=s.length-1; i<j; i++,j--) {
            swap(i,j,s);   
        }    
    }
    public void swap(int i, int j, char[] s) {
        char temp;
        temp=s[i];
        s[i]=s[j];
        s[j]=temp;
    }
}
